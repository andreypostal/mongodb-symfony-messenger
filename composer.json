{
    "name": "andreypostal/symfony-mongodb-messenger",
    "description": "Symfony Mongodb Messenger",
    "type": "library",
    "keywords": [ "symfony", "messenger", "mongo", "mongodb", "transport" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Postal"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-mongodb": "*",
        "symfony/messenger": "^7.0",
        "mongodb/mongodb": "^1.15"
    },
    "autoload": {
        "psr-4": {
            "Andrey\\Symfony\\Messenger\\Mongo\\": "src/"
        },
        "exclude-from-classmap": [
            "tests/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "11.1.x-dev"
    },
    "minimum-stability": "dev"
}
